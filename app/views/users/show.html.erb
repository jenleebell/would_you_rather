<h1>Welcome <%= @user.username %></h1><br>

<h3>Your questions:</h3>
<% @user.questions.each do |question| %>
  <%= link_to question.content, question_path(question) %>
  <br>
<% end %>

<br>
<br>
<h3>Your comments:</h3>
<% @user.comments.each do |comment| %>
  <%= comment.content %>
  <br>
<% end %>
